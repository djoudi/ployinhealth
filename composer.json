{
    "name": "fwartner/ployinhealth",
    "description": "Health Check Service for ployin.co.",
    "keywords": ["framework", "laravel", "lumen", "fwartner", "ployin.co"],
    "license": "MIT",
    "type": "project",

    "author": "Florian Wartner",

    "require": {
        "laravel/lumen-framework": "5.0.*",
        "vlucas/phpdotenv": "~1.0",
        "geerlingguy/ping": "1.x-dev"
    },

    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },

    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Ping\\": "/vendor/geerlingguy/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev"
}
